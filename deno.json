{
  "imports": {
    "@utility/date": "jsr:@utility/date@^0.2.1",
    "hono": "jsr:@hono/hono@^4.6.3"
  },
  "tasks": {
    "start": "deno run -A --env main.ts",
    "dev": "deno run -A --env --watch main.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:wrangler": "wrangler pages dev dist --live-reload",
    "dev:esbuild": "esbuild --bundle src/server.js --format=esm --watch --outfile=dist/_worker.js",
    "build": "esbuild --bundle src/server.js --format=esm --outfile=dist/_worker.js",
    "deploy": "wrangler pages publish dist"
},
}
